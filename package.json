{
  "name": "material-ui-vite-ts",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "npm run cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "clean": "rm -rf .nyc_output || true",
    "report:coverage": "nyc report --reporter=html",
    "instrument": "nyc instrument --compact false src build/src",
    "preinstrument": "npm run clean"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "next",
    "@mui/material": "next",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.12.45",
    "@cypress/vite-dev-server": "5.1.1",
    "@eslint/js": "9.9.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "cypress": "13.13.3",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.10",
    "globals": "15.9.0",
    "istanbul-lib-coverage": "^3.2.2",
    "nyc": "^17.0.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "8.2.0",
    "vite": "5.4.2",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "^2.0.5"
  },
  "nyc": {
    "exclude": [
      "src/models/*.ts"
    ],
    "report-dir": "cypress-coverage",
    "reporter": [
      "html",
      "json"
    ]
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  }
}
